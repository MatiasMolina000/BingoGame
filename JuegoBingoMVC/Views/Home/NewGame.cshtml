@model PartidaModel
@{
    ViewData["Title"] = "Vamos a Jugar!";
}
<h1>@ViewData["Title"]</h1>


<div class="container-fluid">
    <div class="row" id="cartones"></div>   
    <div class="row">
        <div class="col-xl-6 col-lg-12" id="btn-addNumber">
            <button class="nav-link text-dark" onclick="CallNumber()">Lanzar Bolilla</button>
        </div>
    </div>
    <div class="row">
        <div class="col-xl-6 col-lg-12" id="lastNumber">
            Bolilla:
        </div>
        <div class="col-xl-6 col-lg-12" id="winnerNumber">
            Cartón Ganador:
        </div>
    </div>
</div>

<button class="btn btn-primary" onclick="mostrarAlertaSweetAlert2()">Alerta Básica</button>

@section Scripts
{
    <script type="text/javascript" src="~/js/GameJs.js"></script>
}


