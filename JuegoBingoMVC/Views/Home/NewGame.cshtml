@model PartidaModel
@{
    ViewData["Title"] = "Vamos a Jugar!";
}
<h1>@ViewData["Title"]</h1>


<div class="container-fluid">
    <div class="row" id="cartones"></div>   
    <div class="row">
        <div class="col-xl-6 col-lg-12">
            <button class="nav-link text-dark" function="Girar()">Lanzar Bolilla</button>
        </div>
    </div>
    <div class="row">
        <div class="col-xl-6 col-lg-12">
            Bolilla: <p>@ViewData["BolillaCantada"]</p>
        </div>
        <div class="col-xl-6 col-lg-12">
            Cartón Ganador:
        </div>
    </div>
</div>

@section Scripts
{
    <script type="text/javascript" src="~/js/GameJs.js"></script>
}