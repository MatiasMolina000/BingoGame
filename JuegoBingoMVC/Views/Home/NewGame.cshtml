@model PartidaModel
@{
    ViewData["Title"] = "Vamos a Jugar!";
}

<h1>@ViewData["Title"]</h1>

<div class="container-fluid">
    <div class="container">
        <div class="row" id="cartones"></div>
    </div>
       

    <div class="row">
        <div class="col-xl-6 col-lg-12">
            <button id="btn-addNumber" class="btn btn-primary btn-lg" onclick="CallNumber()">Lanzar Bolilla</button>
        </div>
    </div>

    <div class="row data">
        <div class="col-xl-6 col-lg-12" id="lastNumber">Bolilla:</div>
        <div class="col-xl-6 col-lg-12" id="winnerNumber">Cartón Ganador:</div>
    </div>

</div>

@section Scripts
{
    <script type="text/javascript" src="~/js/GameJs.js"></script>
}


